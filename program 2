#include <iostream>
using namespace std;
inline int add(int a, int b) // Inline function definition
{
    return a + b;
}

int main()
{
    int x, y;
    cout << "enter the first no.: ";
    cin >> x;
    cout << "enter the second no.: ";
    cin >> y;
    cout << "Result: " << add(x, y);
    return 0;
}
